{
  "expo": {
    "name": "Number_Guesser",
    "slug": "number_guesser",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "A Simple Mobile App ( Game that guess the number that you choose by yourself  ) Made with React Native using EXPO ",
    "githubUrl": "https://github.com/nawfalhaddi/Number_Guesser"
  }
}
